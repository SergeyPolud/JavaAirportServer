<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entities.Aircrafts" table="aircrafts" schema="airport">
        <id name="aircraftId">
            <column name="AircraftID" sql-type="int"/>
        </id>
        <property name="type">
            <column name="Type" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="regNumber">
            <column name="RegNumber" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="flightId">
            <column name="FlightID" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>