<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entities.Runways" table="runways" schema="airport">
        <id name="runwayId">
            <column name="RunwayID" sql-type="int"/>
        </id>
        <property name="runwayCategory">
            <column name="RunwayCategory" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>